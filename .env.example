# ═══════════════════════════════════════════════════════════════════════════
# AT·OM — EXAMPLE ENVIRONMENT CONFIGURATION
# ═══════════════════════════════════════════════════════════════════════════
# Copy this file to .env and fill in your values
# ⚠️ NEVER commit .env to git — only this .env.example
# ═══════════════════════════════════════════════════════════════════════════

# ═══════════════════════════════════════════════════════════════════════════
# 🔗 HEDERA HASHGRAPH — AT-OM$
# ═══════════════════════════════════════════════════════════════════════════
HEDERA_NETWORK=testnet
HEDERA_ACCOUNT_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE
HEDERA_KEY_TYPE=ED25519
HEDERA_OPERATOR_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_OPERATOR_KEY=YOUR_PRIVATE_KEY_HERE
HEDERA_PUBLIC_KEY=YOUR_PUBLIC_KEY_HERE

# Compte secondaire (optionnel)
MY_ACCOUNT_ID=0.0.YOUR_SECOND_ACCOUNT
MY_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE

# ═══════════════════════════════════════════════════════════════════════════
# 🪙 TOKENS & NFT — AT-OM$
# ═══════════════════════════════════════════════════════════════════════════
HEDERA_TOKEN_ID=0.0.7780104
UR_TOKEN_NAME=AT-OM Tokens
UR_TOKEN_SYMBOL=ATOM
UR_TOKEN_DECIMALS=8

NFT_COLLECTION_ID=0.0.7780274
HEDERA_HCS_TOPIC_ID=
HEDERA_TREASURY_ACCOUNT=0.0.YOUR_TREASURY_ACCOUNT
HEDERA_SIMULATION_MODE=false

# ═══════════════════════════════════════════════════════════════════════════
# 🗄️ DATABASE
# ═══════════════════════════════════════════════════════════════════════════
DATABASE_URL=postgresql+asyncpg://user:password@host:port/dbname?sslmode=require

# ═══════════════════════════════════════════════════════════════════════════
# 🟢 SUPABASE
# ═══════════════════════════════════════════════════════════════════════════
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ═══════════════════════════════════════════════════════════════════════════
# 🔐 AUTHENTICATION
# ═══════════════════════════════════════════════════════════════════════════
JWT_SECRET_KEY=your_jwt_secret
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# ═══════════════════════════════════════════════════════════════════════════
# 🤖 AI PROVIDERS
# ═══════════════════════════════════════════════════════════════════════════
ANTHROPIC_API_KEY=your_anthropic_key
OPENAI_API_KEY=your_openai_key

# ═══════════════════════════════════════════════════════════════════════════
# 🌐 CORS
# ═══════════════════════════════════════════════════════════════════════════
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# ═══════════════════════════════════════════════════════════════════════════
# 📌 PINATA (IPFS)
# ═══════════════════════════════════════════════════════════════════════════
PINATA_GATEWAY=https://gateway.pinata.cloud/ipfs/
PINATA_API_KEY=
PINATA_SECRET_KEY=

# ═══════════════════════════════════════════════════════════════════════════
# 🎵 AT·OM SACRED FREQUENCIES
# ═══════════════════════════════════════════════════════════════════════════
ATOM_M=44.4
ATOM_P=161.8
ATOM_I=369
ATOM_PO=1728
ANCHOR_FREQUENCY=444
SIGNAL_INTERVAL=4.44

# ═══════════════════════════════════════════════════════════════════════════
# 🌐 DOMAINE
# ═══════════════════════════════════════════════════════════════════════════
DOMAIN=at-om.ai
DOMAIN_WWW=www.at-om.ai
