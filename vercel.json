{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "atom-interface",
  "version": 2,
  "framework": "vite",
  "installCommand": "cd Vzwwviru70560-d4e/hardcore-joliot/atom/interface && npm install",
  "buildCommand": "cd Vzwwviru70560-d4e/hardcore-joliot/atom/interface && npm run build",
  "outputDirectory": "Vzwwviru70560-d4e/hardcore-joliot/atom/interface/dist",
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_FREQUENCY": "999",
    "VITE_SOVEREIGN_MODE": "true",
    "VITE_SCEAU_DURATION": "2",
    "VITE_ESSAIM_SIZE": "350",
    "VITE_ZERO_LOG": "true"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|_next|static|manifest.json|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
